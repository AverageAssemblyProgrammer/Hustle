func check_subtraction(a, b) 
    clear()
    printh("checking for 0 as output after subtraction")
    if (a - b) == 0 then 
        printh("Number1 is subtractable by Number2") 
    else 
        printh("Number1 is not subtractable by Number2")
    end
end

func check_devision(x, y)
    printh("checking for 0 as remainder after devision")
    if (x % y) == 0 then 
        printh("Number1 is devisible by Number2") 
    else 
        printh("Number1 is not devisible by Number2")
    end
end